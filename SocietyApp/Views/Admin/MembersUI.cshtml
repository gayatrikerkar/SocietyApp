@model IEnumerable<SocietyApp.Models.Member>

@{
    ViewBag.Title = "MembersUI";
}
@Html.Raw(TempData["InsertMessage"])

@Html.Raw(TempData["UpdateMessage"])
@Html.Raw(TempData["isDeleted"])

@Html.Raw(TempData["DeleteMessage"])


<h2>MembersUI</h2>

<p>
    @Html.ActionLink("Create New Member", "CreateMember")
    @Html.ActionLink("Back to the home page", "Index")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MemID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.Password)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.MemID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.Password)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.MemID }) |
                @Html.ActionLink("Details", "Details", new { id = item.MemID }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.MemID })
            </td>
        </tr>
    }

</table>
